{
  "$schema": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json",
  "adapters": {
    "run_with_debugpy": {
      "command": [
        "${workspaceRoot}/run_with_debugpy"
      ],
      "env": {
        "DEBUG_PORT": "9876"
      },
      "extends": "debugpy",
      "port": 9876
    }
  },
  "configurations": {
    "run": {
      "adapter": "debugpy",
      "breakpoints": {
        "exception": {
          "caught": "N",
          "raised": "N",
          "uncaught": "Y",
          "userUnhandled": "N"
        }
      },
      "configuration": {
        "console": "integratedTerminal",
        "cwd": "${workspaceRoot}",
        "name": "run this Python file",
        "program": "${file}",
        "python": "${workspaceRoot}/venv/Scripts/python.exe",
        "request": "launch",
        "stopOnEntry": false,
        "type": "python"
      }
    },
    "run - main.py": {
      "adapter": "debugpy",
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "",
          "userUnhandled": ""
        }
      },
      "configuration": {
        "console": "integratedTerminal",
        "cwd": "${workspaceRoot}",
        "name": "run main.py",
        "program": "${workspaceRoot}/src/basic_colormath2/main.py",
        "python": "${workspaceRoot}/venv/Scripts/python.exe",
        "request": "launch",
        "stopOnEntry": false,
        "type": "python"
      }
    },
    "test": {
      "adapter": "debugpy",
      "breakpoints": {
        "exception": {
          "caught": "N",
          "raised": "N",
          "uncaught": "Y",
          "userUnhandled": "N"
        }
      },
      "configuration": {
        "args": [
          "-q",
          "${file}"
        ],
        "module": "pytest",
        "name": "run this test file",
        "python": "${workspaceRoot}/venv/Scripts/python.exe",
        "request": "launch",
        "type": "python"
      }
    }
  }
}
